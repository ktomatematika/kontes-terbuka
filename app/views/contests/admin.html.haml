.container
  %h1 Admin: #{@contest.name}
  = link_to 'Kembali', contest_path(@contest), class: 'btn'

  = render 'edit' if can?(:edit, Contest) || can?(:upload_ms, Contest)
  = render 'downloads'
  = render 'misc'
  = render 'short_problems/manage' if can? :manage, ShortProblem
  = render 'long_problems/manage' if can? :manage, LongProblem
  = render 'feedback_questions/manage' if can? :manage, FeedbackQuestion
